{
  "name": "ruse-hooks",
  "version": "1.1.0",
  "description": "react hooks",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "type": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/",
    "esm/"
  ],
  "author": "ziveen",
  "license": "MIT",
  "homepage": "https://github.com/ziveen/use-hooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/ziveen/use-hooks"
  },
  "scripts": {
    "storybook": "start-storybook -p 9001",
    "build-storybook": "build-storybook",
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "deploy": "gh-pages -d storybook-static --git \"$(which git)\"",
    "clean": "yarn rimraf lib esm storybook-static",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-notes": "^5.2.6",
    "@storybook/addon-options": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@types/jest": "^24.0.23",
    "@types/react": "^18.0.12",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "axios": "^0.19.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn clean && yarn build"
    }
  },
  "lint-staged": {
    "src/**.*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "clearMocks": true,
    "coverageDirectory": "coverage",
    "testMatch": [
      "<rootDir>/test/**/*.test.(ts|tsx)"
    ]
  }
}
